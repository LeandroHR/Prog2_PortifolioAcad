public class Perfil {

	public String nome;
	public String user;

	public Perfil(String nome, String user){
		this.nome = nome;
		this.user = user;
	}
	
	public Perfil()
	{
		
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public String getUser() {
		return user;
	}

	public void setUser(String user) {
		this.user = user;
	}

}


