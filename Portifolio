import java.util.ArrayList;


public class Portifolio {
	private ArrayList <Trabalho> t = new ArrayList<Trabalho>();
	private ArrayList <Projeto> p = new ArrayList <Projeto>();
	
	public Portifolio()
	{
		
	}
	
	public Portifolio(ArrayList <Trabalho> t ,ArrayList <Projeto> p)
	{
		this.t = t;
		this.p = p;
	}
	
	public ArrayList<Trabalho> getT() {
		return t;
	}
	public void addTrabalho(Trabalho t) {
		this.t.add(t);
	}
	public ArrayList<Projeto> getP() {
		return p;
	}
	public void addProjeto(Projeto p) {
		this.p.add(p);
	}
}
